{
  "name": "logo-fetch",
  "version": "1.0.0",
  "description": "Instantly retrieve a company logo given a company name",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "logo-fetch": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:web": "tsc -p tsconfig.web.json",
    "dev": "node --watch dist-web/server.js",
    "start": "node dist-web/server.js",
    "test": "node --test dist/**/*.test.js"
  },
  "keywords": [
    "logo",
    "company",
    "brand",
    "favicon",
    "avatar"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^7.0.1",
    "brand.dev": "^0.25.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/express": "^5.0.2",
    "@types/node": "^25.2.2",
    "@vercel/node": "^5.5.33",
    "typescript": "^5.9.3"
  }
}
